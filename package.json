{
  "name": "gitlab-reviewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "axios": "^0.19.0",
    "favico.js": "^0.3.10",
    "node-sass": "^4.12.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "yarn run watch:css & react-scripts start",
    "build": "yarn run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/styles/index.scss -o src/styles/main.scss",
    "watch:css": "postcss src/styles/index.scss -o src/styles/main.scss -w",
    "type-check": "yarn tsc --project .",
    "lint": "tslint --project .",
    "lint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier-tslint fix 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/favico.js": "^0.0.28",
    "autoprefixer": "^9.6.0",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "tailwindcss": "^1.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.0.0"
  }
}
