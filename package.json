    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.17",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.19.0",
    "favico.js": "^0.3.10",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-emojione": "^5.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "3.7.3"
  },
  "scripts": {
    "start": "yarn run watch:css & react-scripts start",
    "build": "yarn run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/styles/index.scss -o src/styles/main.scss",
    "watch:css": "postcss src/styles/index.scss -o src/styles/main.scss -w",
    "type-check": "yarn tsc --project .",
    "lint": "tslint --project .",
    "lint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier-tslint fix 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@types/favico.js": "^0.0.28",
    "autoprefixer": "^9.7.3",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "tailwindcss": "^1.1.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.1.0"
  }
